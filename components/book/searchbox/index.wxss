.content {
  display: flex;
  justify-content: center;
  background-color: #fff;
  width: 100%;
  padding: 25rpx 0;
}
.content .searbox {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #999999;
  background-color: #f7f7f7;
  border-radius: 32rpx ;
  width: 92%;
  height: 64rpx;
}
.content .searbox .iconfont {
  margin-right: 15rpx;
  font-size: 34rpx;
}
.content .searbox .searchinfo {
  font-size: 26rpx;
}
